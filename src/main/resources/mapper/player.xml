<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.prac.demo.repository.PlayerRepository">
  <select id="findByTeam" resultType="com.prac.demo.model.Player">
    select * from Player where pTeamId = #{id}
  </select>
  
  <select id="findById" resultType="com.prac.demo.model.Player">
    select * from Player where id = #{id}
  </select> 
 
</mapper>
